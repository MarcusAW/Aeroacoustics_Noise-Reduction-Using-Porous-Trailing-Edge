FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.19e2;	

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    airfoil
    {
        type            omegaWallFunction;
        value           $internalField;;
    }
    "(top|bottom)"
    {
        type            zeroGradient;
    }
    
    frontCyc
    {
        type            cyclicAMI;
    }
    backCyc
    {
        type            cyclicAMI;
    }
}

// ************************************************************************* //
